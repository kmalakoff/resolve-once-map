{
  "name": "resolve-once-map",
  "version": "0.6.9",
  "description": "Resolves a promise only once and memoizes the result in a map",
  "keywords": [
    "promise",
    "resolve",
    "once",
    "memoize",
    "resolve-once"
  ],
  "homepage": "https://github.com/kmalakoff/resolve-once-map",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/resolve-once-map.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "resolve-once": "^0.6.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5",
    "pinkie-promise": "^2.0.1"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts",
    "targets": [
      "cjs"
    ]
  }
}
